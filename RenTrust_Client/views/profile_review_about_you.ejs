<main id="site-content" role="main" ng-controller="review_controller" ng-init="loadReviewAboutPage()">

    <div class="subnav hide-print">
        <div class="page-container-responsive">
            <ul class="subnav-list">
                <li>
                    <a href="/getDashBoardPage" aria-selected="false" class="subnav-item">Dashboard</a>
                </li>
                <li>
                    <a href="/yourListings" aria-selected="false" class="subnav-item">Your Listings</a>
                </li>
                <li>
                    <a href="/yourTrips" aria-selected="false" class="subnav-item">Your Trips</a>
                </li>
                <li>
                    <a href="#" aria-selected="true" class="subnav-item">Profile</a>
                </li>
                <li>
                    <a href="/Account_Transactions" aria-selected="false" class="subnav-item">Account</a>
                </li>
            </ul>
        </div>
    </div>


    <div id="notification-area"></div>
    <div class="page-container-responsive space-top-4 space-4">
        <div class="row">
            <div class="col-md-3 space-sm-4">
                <div class="sidenav">
                    <ul class="sidenav-list">
                        <li>
                            <a href="/editProfilePage" aria-selected="false" class="sidenav-item">Edit Profile</a>
                        </li>
                        <li>
                            <a href="/getUserPhotoPage" aria-selected="false" class="sidenav-item">Photos</a>
                        </li>
                        <li>
                            <a href="#" aria-selected="true" class="sidenav-item">Reviews</a>
                        </li>
                    </ul>
                </div>
                <a href="/profile/" class="btn btn-block row-space-top-4" target="_blank">View Profile</a>
            </div>
            <div class="col-md-9">
                <ul role="tablist" class="tabs">
                    <li>
                        <a aria-selected="true" aria-controls="received" role="tab" href="" class="tab-item">
                            Reviews About You
                        </a>
                    </li>
                    <li>
                        <a aria-selected="false" aria-controls="sent" role="tab" class="tab-item"
                           href="/getUserReviewbyPage">
                            Reviews By You
                        </a>
                    </li>
                </ul>

                <div id="dashboard-content">

                    <div class="row-space-top-4" id="reviews">
                        <div aria-hidden="false" id="received" role="tabpanel" class="tab-panel" style="">
                            <div class="panel">
                                <div class="panel-header">
                                    Reviews from User
                                </div>
                                <div class="panel-body">
                                    <p class="text-lead">
                                        You will see all past reviews here.
                                    </p>
                                    <ul class="list-layout reviews-list row-space-top-4">
                                        <li class="media reviews-list-item row-space-top-2"
                                            ng-repeat="fromuser in fromUserReview">
                                            <div class="pull-left text-center">
                                                <a class="media-photo media-round" target="_blank"
                                                   href="/profile/{{fromuser.userId.userId}}">
                                                    <img width="68" height="68"
                                                         src="/images/{{fromuser.userId.profileImage}}">
                                                </a>
                                                <div class="name"><a class="text-muted" target="_blank"
                                                                     href="/profile/{{fromuser.userId.userId}}">{{fromuser.userId.firstName}}
                                                        {{fromuser.userId.lastName}}</a></div>
                                            </div>
                                            <div class="media-body response">
                                                <p>{{fromuser.review}}</p>
                                                <hr>
                                                <div class="row-space-top-2">
                                                    <p>
                                                        <strong>
                                                            <i aria-label="Private" data-behavior="tooltip"
                                                               class="icon icon-lock icon-rausch"></i>
                                                            Rating: {{fromuser.rating}}
                                                        </strong>
                                                        <br>
                                                    </p>
                                                </div>
                                                <p class="clearfix text-muted">
                                                    date: {{fromuser.createdDate | date: "shortDate"}}
                                                </p>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row-space-top-4" id="reviews">
                        <div aria-hidden="false" id="received" role="tabpanel" class="tab-panel" style="">
                            <div class="panel">
                                <div class="panel-header">
                                    Reviews from Host
                                </div>
                                <div class="panel-body">
                                    <p class="text-lead">
                                        You will see all past reviews here.
                                    </p>
                                    <ul class="list-layout reviews-list row-space-top-4">
                                        <li class="media reviews-list-item row-space-top-2"
                                            ng-repeat="fromhost in fromHostReview">
                                            <div class="pull-left text-center">
                                                <a class="media-photo media-round"
                                                   href="/profile/{{fromhost.hostId.userId}}" target="_blank">
                                                    <img width="68" height="68" title=""
                                                         src="images/{{fromhost.hostId.profileImage}}" alt="">
                                                </a>
                                                <div class="name"><a class="" target="_blank"
                                                                     href="/profile/{{fromhost.hostId.userId}}">{{fromhost.hostId.firstName}}
                                                        {{fromhost.hostId.lastName}}</a></div>
                                            </div>
                                            <div class="media-body response">
                                                <p>{{fromhost.review}}</p>
                                                <hr>

                                                <div class="row-space-top-2">
                                                    <p>
                                                        <strong>
                                                            <i aria-label="Private" data-behavior="tooltip"
                                                               class="icon icon-lock icon-rausch"></i>
                                                            Rating: {{fromhost.rating}}
                                                        </strong>
                                                        <br>

                                                    </p>
                                                </div>
                                                <p class="clearfix text-muted">
                                                    date: {{fromhost.createdDate | date: "shortDate"}}
                                                </p>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</main>

