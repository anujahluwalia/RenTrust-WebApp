                        </div>
                    </div>
                    <div class="panel-body">
                        <h2 class="text-center">
                            <%= firstname %>
                        </h2>
                        <ul class="list-unstyled text-center">
                            <li>
                                <a href="/getUserPhotoPage">View Profile</a>
                            </li>
                            <li>
                                <a href="/editProfilePage" class="btn btn-primary btn-block text-wrap space-top-1"
