                    </li>
                    <li>
                        <a href="/Account_Security" aria-selected="false" class="sidenav-item">Security</a>
                    </li>
                </ul>
            </div>
            <div class="col-md-9">
                <div class="" id="payout_setup">
                    <div class="panel row-space-4">
                        <div class="panel-header">
                            Payment Methods
                        </div>
                        <div class="panel-body" id="payout_intro">
                            <p>
                                Your credit card information
                            </p>
                            <div class="container" ng-controller="account_user_management">
                                <form class="form-horizontal" ng-submit="creditCard()" name="checkFrom" id="myForm"
                                      ng-submit="checkoutFinal()">
                                    <fieldset>
                                        <legend align="center">Payment</legend>
                                        <br>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="card-holder-name">Name on
                                                Card</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" name="cname"
                                                       ng-model="cname" id="card-holder-name"
                                                       placeholder="Card Holder's Name" required>
                                                <br>
                                            </div>
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="card-number">Card Number</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" name="cnum" ng-model="cnum"
                                                       ng-pattern="/^(?:4[0-9]{12}(?:[0-9]{3})?|5[1-5][0-9]{14}|6(?:011|5[0-9][0-9])[0-9]{12}|3[47][0-9]{13}|3(?:0[0-5]|[68][0-9])[0-9]{11}|(?:2131|1800|35\d{3})\d{11})$/"
                                                       id="card-number" placeholder="Debit/Credit Card Number" required>
                                                <br>
                                            </div>
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="expiry-month">Expiration
                                                Date</label>
                                            <div class="col-sm-9">
                                                <div class="col-xs-3">
                                                    <select class="form-control col-sm-2" ng-model="expMonth"
                                                            name="expiryMonth" id="expiry-month" required>
                                                        <option value="01">Jan (01)</option>
                                                        <option value="02">Feb (02)</option>
                                                        <option value="03">Mar (03)</option>
                                                        <option value="04">Apr (04)</option>
                                                        <option value="05">May (05)</option>
                                                        <option value="06">June(06)</option>
                                                        <option value="07">July(07)</option>
                                                        <option value="08">Aug (08)</option>
                                                        <option value="09">Sep (09)</option>
                                                        <option value="10">Oct (10)</option>
                                                        <option value="11">Nov (11)</option>
                                                        <option value="12" selected="selected">Dec (12)</option>
                                                    </select>
                                                </div>
                                                <div class="col-xs-3">
                                                    <select class="form-control col-sm-2" name="expiryYear"
                                                            ng-model="expYear" required>
                                                        <option value="2017">2017</option>
                                                        <option value="2018">2018</option>
                                                        <option value="2019">2019</option>
                                                        <option value="2020">2020</option>
                                                        <option value="2021">2021</option>
                                                        <option value="2022">2022</option>
                                                        <option value="2023">2023</option>
                                                        <option value="2024">2024</option>
                                                        <option value="2025">2025</option>
                                                        <option value="2026">2026</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="cvv">Card CVV</label>
                                            <div class="col-sm-3">
                                                <br>
                                                <input type="number" class="form-control" min="100" max="999" name="ccv"
                                                       ng-model="ccv" id="cvv" placeholder="Security Code" required>
                                                <br>
                                            </div>
                                        </div>
                                        <br>
                                        <div class="form-group"
                                             ng-show="!checkFrom.cname.$invalid && !checkFrom.cnum.$invalid && !checkFrom.ccv.$invalid">
                                            <div class="col-sm-offset-3 col-sm-9">
                                                <input type="submit" class="btn btn-primary" value="Add Payout Method"
                                                       id="add-payout-method-button"/>
                                                <br>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-offset-3 col-sm-9">
                                                <div class="form-group"
                                                     ng-show="checkFrom.ccv.$touched && checkFrom.ccv.$invalid">
                                                    <br>
                                                    <span style="color: red">Invalid Credit Card CVV</span>
                                                </div>
                                                <div class="form-group"
                                                     ng-show="checkFrom.cnum.$touched && checkFrom.cnum.$invalid && checkFrom.cnum.$error.pattern">
                                                    <br>
                                                    <span style="color: red">Invalid Credit Card number</span>
                                                </div>
                                                <div class="form-group"
                                                     ng-show="checkFrom.cname.$touched && checkFrom.cname.$invalid">
                                                    <br>
                                                    <span style="color: red">Enter Credit Card Holder Name</span>
                                                </div>
                                                <div class="alert alert-success" ng-show=alert2>
                                                    Payment Method Success Fully Updated
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div style="display:none;" class="add_payout_section" id="payout_new_select"></div>
                    <div style="display:none;" class="add_payout_section" id="payout_edit"></div>
                </div>
                <div id="taxes"></div>
            </div>
        </div>
    </div>
</main>

